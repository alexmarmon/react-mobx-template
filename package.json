{
  "name": "react-mobx-template",
  "version": "1.1.0",
  "description": "Template with React, MobX, React Router 4, Webpack 3, Hot Reload 3, Airbnb's ESLint, and an Express API",
  "author": "Alex Marmon",
  "license": "MIT",
  "repository": "https://github.com/alexmarmon/react-mobx-template.git",
  "scripts": {
    "postreceive": "echo 'it ran' > ~/home/sites/react-mobx-template/test.txt; npm run build; pm2 restart template;",
    "precommit": "echo 'woahhhh'",
    "dev": "node server.js",
    "lint": "eslint --ext .js --ext .jsx src/; exit 0",
    "build": "webpack --progress",
    "test": "mocha src/tests/.setup.js src/tests/*test.jsx --timeout 10000; exit 0",
    "production": "node server.js",
    "landing-page": "mv src/pages/home/ src/; rm -rf src/pages/; rm -rf src/shared_modules/; sed -i -e '4d;6d;19d;20d;' src/app.jsx; sed -ie 's/Header/Home/' src/app.jsx; sed -ie 's/\\/pages\\/home/\\/home/' src/app.jsx; rm src/app.jsx-e; rm src/app.jsxe; sed -ie 's/\\.\\.\\/\\.\\.\\//\\.\\.\\//' src/home/Home.jsx; sed -ie 's/\\.\\.\\/\\.\\.\\/\\.\\.\\/\\.\\.\\//\\.\\.\\/\\.\\.\\/\\.\\.\\//g' src/home/modules/title/title.scss; rm src/home/modules/title/title.scsse; rm src/home/Home.jsxe; sed -i -e '7d;12d' src/home/Home.jsx; sed -ie 's/User/Title/' src/home/Home.jsx; rm src/home/Home.jsx-e"
  },
  "keywords": [
    "react",
    "mobx",
    "hot reload 3",
    "template",
    "starter-kit",
    "react router 4",
    "webpack 3"
  ],
  "eslintConfig": {
    "extends": "airbnb",
    "parser": "babel-eslint",
    "globals": {
      "document": true,
      "fetch": true,
      "console": true,
      "it": true,
      "describe": true,
      "window": true
    },
    "rules": {
      "no-console": 0,
      "window": 0,
      "jsx-a11y/no-static-element-interactions": 0,
      "radix": 0,
      "forbid-prop-types": 0,
      "react/forbid-prop-types": 0,
      "no-nested-ternary": 0,
      "max-len": 0,
      "react/require-default-props": 0,
      "react/jsx-wrap-multilines": 0
    }
  },
  "babel": {
    "presets": [
      "react",
      "stage-1",
      [
        "es2015",
        {
          "modules": false
        }
      ]
    ],
    "plugins": [
      "transform-decorators-legacy",
      "transform-class-properties",
      "react-hot-loader/babel"
    ]
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.1.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015-native-modules": "^6.9.4",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "babel-register": "^6.24.1",
    "copy-webpack-plugin": "^4.0.1",
    "css-loader": "^0.28.0",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.0.1",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^5.0.3",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.1",
    "jsdom": "^11.0.0",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.2",
    "react-addons-test-utils": "^15.5.1",
    "sass-loader": "^6.0.3",
    "style-loader": "^0.18.2"
  },
  "dependencies": {
    "body-parser": "^1.15.2",
    "chai": "^4.0.2",
    "chai-http": "^3.0.0",
    "compression": "^1.6.2",
    "enzyme": "^2.8.2",
    "express": "^4.15.2",
    "isomorphic-fetch": "^2.2.1",
    "knex": "^0.13.0",
    "lofty-pretty-print-plugin": "^1.0.0",
    "mobx": "^3.1.9",
    "mobx-react": "^4.1.8",
    "mysql": "^2.11.1",
    "ora": "^1.3.0",
    "prop-types": "^15.5.8",
    "react": "^15.5.4",
    "react-dom": "^15.5.4",
    "react-hot-loader": "next",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-test-renderer": "^15.5.4",
    "webpack": "^3.0.0",
    "webpack-dev-server": "^2.5.0",
    "whatwg-fetch": "^2.0.3"
  }
}
