{
  "name": "react-mobx-template",
  "version": "2.0.0",
  "description": "Template with React, MobX, React Router 4, Webpack 3, Airbnb's ESLint, and an Express API",
  "author": "Alex Marmon",
  "license": "MIT",
  "repository": "https://github.com/alexmarmon/react-mobx-template.git",
  "scripts": {
    "dev": "cross-env BABEL_ENV=development PORT=4000 node config/dev-server.js",
    "lint": "eslint --ext .js --ext .jsx src/ api/; exit 0",
    "lint-fix": "eslint --fix --ext .js --ext .jsx src/ api/; exit 0",
    "build": "cross-env PORT=4000 BABEL_ENV=production webpack --config config/webpack.config.prod.js --progress --colors",
    "test": "cross-env PORT=4000 node_modules/.bin/ava src/tests",
    "production": "cross-env PORT=4000 node config/prod-server.js"
  },
  "keywords": [
    "react",
    "mobx",
    "template",
    "starter-kit",
    "react router 4",
    "webpack 3"
  ],
  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.4",
    "babel-plugin-module-resolver": "^3.1.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.18.2",
    "chalk": "^2.3.2",
    "chokidar": "^2.0.3",
    "cli-spinners": "^1.1.0",
    "compression": "^1.7.2",
    "copy-webpack-plugin": "^4.5.1",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "draftlog": "^1.0.12",
    "enzyme": "^3.3.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-import-resolver-webpack": "^0.8.4",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.7.0",
    "express": "^4.16.3",
    "html-webpack-plugin": "^3.1.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^11.6.2",
    "knex": "^0.14.4",
    "mini-css-extract-plugin": "^0.2.0",
    "mobx": "^4.1.0",
    "mobx-react": "^5.0.0",
    "mysql": "^2.15.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.3",
    "prop-types": "^15.6.1",
    "react": "^16.2.0",
    "react-addons-test-utils": "^15.6.2",
    "react-dom": "^16.2.0",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-test-renderer": "^16.2.0",
    "sass-loader": "^6.0.7",
    "style-loader": "^0.20.3",
    "supertest": "^3.0.0",
    "webpack": "^4.2.0",
    "webpack-cli": "^2.0.13",
    "webpack-dev-server": "^3.1.1",
    "webpack-hot-middleware": "^2.21.2",
    "webpack-merge": "^4.1.2"
  }
}
